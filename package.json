{
  "name": "shadcn-admin",
  "private": false,
  "version": "2.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "bun run tsc -b && vite build",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:fix": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "preview": "vite preview",
    "gen:routes": "tsr generate",
    "gen:api": "openapi-ts",
    "gen:api:bun": "OPENAPI_FILE=http://localhost:3000/openapi.json openapi-ts",
    "gen:api:watch": "openapi-ts --watch",
    "gen:api:check": "openapi-ts && git diff --exit-code src/client/",
    "knip": "knip",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "ci": "bun run check && bun test",
    "ci:full": "bun run gen:api:check && bun run check && bun test",
    "i18n:test": "bun run src/i18n-consistency.test.ts",
    "i18n:lint": "i18next-cli lint",
    "i18n:extract": "i18next-cli extract",
    "i18n:extract:watch": "i18next-cli extract --watch",
    "i18n:sync": "i18next-cli sync",
    "i18n:watch": "i18next-cli watch",
    "i18n:types": "i18next-cli types",
    "i18n:status": "i18next-cli status"
